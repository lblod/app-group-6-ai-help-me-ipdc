          "naam": {
            "nl-BE-x-generated-informal": "Uitdoving woonbonus vanaf 1 januari 2020 - FAQ",
            "nl-BE-x-formal": "Uitdoving woonbonus vanaf 1 januari 2020 - FAQ",
            "nl": "Uitdoving woonbonus vanaf 1 januari 2020 - FAQ"
