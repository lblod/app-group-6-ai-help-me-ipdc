        {
          "naam": {
            "nl-BE-x-generated-informal": "Premie voor zonnepanelen (Fluvius)",
            "nl-BE-x-generated-formal": "Premie voor zonnepanelen (Fluvius)",
            "nl": "Premie voor zonnepanelen (Fluvius)"
          },
          "url": "https://www.fluvius.be/nl/thema/premies/premies-voor-huishoudelijke-klanten/premie-zonnepanelen",
          "@type": "Website",
